{
    "name": "react-leaflet-vectorgrid",
    "version": "2.1.3",
    "description": "React wrapper of Leaflet.VectorGrid. Display gridded vector data (sliced GeoJSON, TopoJSON or protobuf vector tiles).",
    "main": "dist/react-leaflet-vectorgrid.min.js",
    "scripts": {
        "build": "npm run build:lib && npm run build:dist",
        "build:dist": "webpack src/index.js dist/react-leaflet-vectorgrid.js & webpack -p src/index.js dist/react-leaflet-vectorgrid.min.js",
        "build:lib": "npm run compile:lib",
        "clean:lib": "rm -Rf ./lib",
        "compile:lib": "babel src --out-dir lib",
        "prepublishOnly": "npm run build",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mhasbie/react-leaflet-vectorgrid.git"
    },
    "keywords": [
        "react",
        "leaflet",
        "react-leaflet",
        "leaflet.vectorgrid",
        "vector-tile",
        "geojson-vt",
        "pbf",
        "topojson",
        "react-leaflet-vectorgrid"
    ],
    "author": "M. Hasbie",
    "license": "MIT",
    "peerDependencies": {
        "leaflet": "^1.3.4",
        "react-leaflet": "^2.0.1",
        "debug": "^4.1.0"
    },
    "devDependencies": {
        "babel-cli": "^6.8.0",
        "babel-core": "^6.8.0",
        "babel-eslint": "^7.0.0",
        "babel-loader": "^6.2.4",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-react": "^6.0.15",
        "babel-preset-stage-0": "^6.1.18",
        "babel-register": "^6.26.0",
        "d3": "4.10.0",
        "debug": "^4.1.0",
        "eslint": "^3.8.1",
        "eslint-plugin-react": "^6.4.1",
        "leaflet": "^1.3.4",
        "macaddress": "^0.2.9",
        "mime": "^1.6.0",
        "prop-types": "^15.5.10",
        "react": "^16.5.2",
        "react-dom": "^16.5.2",
        "react-leaflet": "^2.0.1",
        "webpack": "^1.13.0",
        "webpack-css-loaders": "^1.0.0",
        "webpack-dev-server": "^1.16.5"
    },
    "dependencies": {
        "leaflet.vectorgrid": "^1.3.0"
    }
}
